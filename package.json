{
  "name": "UI-Docs-Server",
  "version": "1.0.0",
  "description": "Server for UI-Docs",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "author": "xiefei",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./app",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "babel-node index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-plugin-import": "^1.1.0",
    "core-js": "^2.4.0",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-static": "^2.0.0",
    "koa-Router": "^1.0.4",
    "koa-better-router": "^2.0.1",
    "koa-body": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-router": "^7.0.1",
    "npm": "^4.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.5",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.7",
    "cheerio": "^0.20.0",
    "debug": "^2.2.0",
    "eslint": "^2.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.2.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.1",
    "fs-extra": "^0.26.3",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.1",
    "yargs": "^4.0.0"
  }
}
